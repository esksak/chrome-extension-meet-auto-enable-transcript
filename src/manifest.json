{
  "manifest_version": 3,
  "name": "Google Meet 文字起こし自動開始",
  "version": "1.0.8",
  "description": "Google Meetで自動的に文字起こしを開始します",
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
